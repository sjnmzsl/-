<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yanx">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.YanXuan">
        <activity android:name=".mvp.register.RegisterActivity"></activity>
        <activity android:name=".mvp.BuyShopping.BuyShoppingActivity" />
        <activity android:name=".mvp.details.DetailsActivity" />
        <activity android:name=".mvp.start.view.StartPageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mvp.login.view.LoginActivity" />
        <activity android:name=".mvp.home.activity.HomeActivity" />
    </application>

</manifest>